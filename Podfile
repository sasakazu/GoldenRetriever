  pod 'Firebase/Storage'
  pod 'FirebaseUI'
  pod 'FirebaseUI/Storage'



  target 'GoldenRetrieverTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'GoldenRetrieverUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
